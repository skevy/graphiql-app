{
  "name": "graphiql-app",
  "productName": "GraphiQL",
  "version": "0.7.2",
  "description": "A light, Electron-based wrapper around GraphiQL.",
  "main": "main.js",
  "scripts": {
    "build-dev": "webpack --config webpack/webpack.config.js --progress",
    "build": "webpack --mode=production --node-env=production --config webpack/webpack.config.production.js",
    "start": "electron .",
    "start-dev": "NODE_ENV=development electron .",
    "package": "electron-builder build --dir",
    "dist": "electron-builder build -mwl",
    "dist:clean": "rm -rf ./dist ./release",
    "dist:linux": "electron-builder build -l",
    "release": "yarn dist-clean && yarn build && electron-builder build -mwl",
    "test": "npm run build",
    "postinstall": "electron-builder install-app-deps",
    "watch": "NODE_ENV=development electron . & webpack --watch --config webpack/webpack.config.js"
  },
  "browserslist": "> 0.25%, not dead",
  "author": "Adam Miskiewicz <adam.skevy@mac.com> (http://github.com/skevy)",
  "license": "MIT",
  "homepage": "https://github.com/skevy/graphiql-app",
  "bugs": {
    "url": "https://github.com/skevy/graphiql-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/skevy/graphiql-app.git"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.14.3",
    "crypto-browserify": "^3.12.0",
    "graphiql": "^1.4.2",
    "graphql": "^15.5.0",
    "lodash": "^4.17.21",
    "mousetrap": "^1.6.5",
    "object-assign": "^4.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-modal": "^3.13.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-flow": "^7.13.13",
    "@babel/preset-react": "^7.13.13",
    "asar": "^3.0.3",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^5.2.6",
    "del": "^6.0.0",
    "electron": "^11.0.0",
    "electron-builder": "^22.10.5",
    "electron-packager": "^15.2.0",
    "electron-reloader": "^1.2.1",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.2.0",
    "github-latest-release": "^0.1.1",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.2.0",
    "ignore-loader": "^0.1.2",
    "json-loader": "^0.5.7",
    "loaders-by-extension": "^1.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "minimist": "^1.2.5",
    "mocha": "^8.4.0",
    "raw-loader": "^4.0.2",
    "react-hot-loader": "^4.13.0",
    "spectron": "^15.0.0",
    "stats-webpack-plugin": "^0.7.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "build": {
    "directories": {
      "output": "release"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "skevy",
        "repo": "graphiql-app"
      }
    ],
    "asar": true,
    "appId": "com.sk3vy.graphiql-app",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 168,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 104,
          "y": 168,
          "type": "file"
        }
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "category": "Development",
      "target": [
        "AppImage"
      ]
    },
    "nsis": {
      "perMachine": true
    }
  }
}
